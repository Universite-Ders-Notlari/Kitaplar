\par }{\b\f0\fs24 \'d6rnek B.1: }{\f68\fs24 S\'fdral\'fd ba\'f0l\'fd listenin ger\'e7ekle\'fetirilmesi.}{\b\f0\fs24 
\par }{\f0 
\par #include <stdlib.h>
\par #include <string.h>
\par struct listyapi \{
\par     char   adi[21];
\par     struct listyapi *sonraki;
\par \};
\par typedef struct listyapi listnode;
\par typedef listnode *listptr;
\par }{\f64 listptr headnode;   /* Herzaman listenin ba\'fe\'fdn\'fd g\'f6sterir */
\par }{\f0 
\par }{\b\f0 void seeklist(char *searchthis, listptr *prevnode)
\par }{\f0 \{
\par   listptr c;
\par 
\par   c = headnode;
\par   *prevnode = c;
\par   while ((c->sonraki != NULL)) \{
\par        c = c->sonraki;
\par        if (strcmp(c->adi, searchthis) >= 0) break;
\par        *prevnode = c;
\par   \}
\par \}
\par 
\par }{\b\f0 void kayit(char *s)
\par }{\f68 /* prevnode kay\'fdd\'fd newnode kay\'fdd\'fdn\'fd, newnode kay\'fdd\'fd prevnode'nin daha \'f6nce g\'f6sterdi\'f0i kay\'fdd\'fdn\'fd g\'f6sterir. */
\par }{\f0 \{
\par    listptr newnode, prevnode;
\par 
\par }{\f68    newnode = malloc(sizeof(listnode)); /* yeni kay\'fdda yer a\'87 */
\par }{\f0 
\par }{\f68    strcpy(newnode->adi, s);            /*bilgiyi yeni kay\'fdda yaz */
\par }{\f0 
\par    seeklist(newnode->adi, &prevnode);  /* listedeki yerini bul */
\par 
\par    newnode->sonraki = prevnode->sonraki;  /* listeye ekle */
\par    prevnode->sonraki = newnode;
\par \}
\par 
\par }{\b\f0 void iptal(char *s)
\par }{\f68 /* newnode kay\'fdd\'fd silinir. prevnode kay\'fdd\'fd newnode kay\'fdd\'fdn\'fdn g\'f6sterdi\'f0i kay\'fdd\'fdn\'fd g\'f6sterir. */
\par }{\f0 \{
\par    listptr newnode, prevnode;
\par 
\par    seeklist(s, &prevnode);
\par    newnode = prevnode->sonraki;
\par    prevnode->sonraki = newnode->sonraki;
\par    free(newnode);
\par \}
\par 
\par }{\b\f0 void listlist(void)
\par }{\f0 \{ 
\par    listptr currentnode;
\par 
\par    currentnode = headnode;
\par    if (currentnode != NULL) currentnode = currentnode->sonraki;
\par    while (currentnode != NULL)
\par        \{
\par \tab   printf("%s  ",currentnode->adi);
\par \tab   currentnode = currentnode->sonraki;
\par        \}
\par    printf("\\n");
\par 
\par \}   /* Procedure listlist; */
\par 
\par }{\b\f0 main()
\par }{\f0 \{
\par   char      sec;
\par   char      *s;
\par 
\par   headnode = malloc(sizeof(*headnode));
\par }{\f64   strcpy(headnode->adi," listenin ba\'fe\'fd");
\par }{\f0   headnode->sonraki = NULL;
\par 
\par   do \{
\par     clrscr();
\par }{\f68     printf("Bo\'fe yer : %ld\\n\\n",coreleft());
\par }{\f0 
\par     listlist();
\par 
\par }{\f68     printf("\\n\\n1 - Giri\'fe\\n2 - iptal\\n3 - Son\\n\\nSe\'e7 \\n");
\par }{\f0 
\par     sec = getch();
\par     switch (sec) \{
\par       case '1':
\par }{\f68 \tab \tab printf("Ad\'fd "); gets(s);
\par }{\f0 \tab \tab kayit(s);
\par \tab \tab break;
\par 
\par }{\f68       case '2': printf("Ad\'fd \\n"); gets(s);
\par }{\f0 \tab \tab iptal(s);
\par \tab \tab break;
\par 
\par       case '3': exit(0);break;
\par     \}
\par   \}  while (1);
\par \}
