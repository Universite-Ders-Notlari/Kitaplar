\par }\pard \s16\nowidctlpar\widctlpar\adjustright {
\par typedef struct \{
\par   float re, im;
\par \} complex;
\par 
\par main()
\par \{
\par   complex topla(complex, complex);
\par   complex cikart(complex, complex);
\par   complex carp(complex, complex);
\par 
\par   complex z1=\{1,1\}, z2=\{2,2\};
\par   complex z3;
\par 
\par   clrscr();
\par   printf("%3.0f %3.0fj   %3.0f %3.0fj\\n",z1.re,z1.im,z2.re,z2.im);
\par 
\par   z3 = topla(z1, z2);
\par   printf("%3.0f  %3.0f\\n",z3.re,z3.im);
\par 
\par   z3 = cikart(z1, z2);
\par   printf("%3.0f  %3.0f\\n",z3.re,z3.im);
\par 
\par   z3 = carp(z1, z2);
\par   printf("%3.0f  %3.0f\\n",z3.re,z3.im);
\par   getch();
\par \}
\par }\pard \s16\sl360\slmult0\nowidctlpar\widctlpar\adjustright {complex topla(complex a, complex b)
\par }\pard \s16\nowidctlpar\widctlpar\adjustright {\{
\par  complex z;
\par  z.re = a.re + b.re;
\par  z.im = a.im + b.im;
\par  return z;
\par \}
\par complex cikart(complex a, complex b)
\par \{
\par  complex z;
\par  z.re = a.re - b.re;
\par  z.im = a.im - b.im;
\par  return z;
\par \}
\par complex carp(complex a, complex b)
\par \{
\par  complex z;
\par  z.re = a.re * b.re - a.im * b.im;
\par  z.im = a.re * b.im + a.im * b.re;
\par  return z;
\par \}
\par }\pard \s16\sl360\slmult0\nowidctlpar\widctlpar\adjustright {
