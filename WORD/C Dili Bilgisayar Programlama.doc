\par }{\b\f0 void push(char *gelen)
\par }{\f0 \{
\par   stackptr newnode;
\par   newnode = malloc(sizeof(stacknode));
\par   strcpy(newnode->deger, gelen);
\par   newnode->sonraki = top;
\par   top = newnode;
\par \}
\par 
\par }{\b\f0 int pop(char *giden)
\par }{\f0 \{
\par    stackptr oldnode;
\par 
\par    if (top != NULL) \{
\par       oldnode = top;
\par       strcpy(giden, top->deger);
\par       top = top->sonraki;
\par       free(oldnode);
\par       return 0;
\par    \}
\par    else
\par }{\f68       return 1; /* y\'fd\'f0\'fdn bo\'fe */
\par }{\f0 \}
