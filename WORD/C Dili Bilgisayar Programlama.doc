\par }{\b \'d6rnek 8.1.8:}{\f68  A\'fea\'f0\'fddaki i\'felevlerin ger\'e7ekle\'fetirilmesi.
\par }{
\par }{\f68 void binary_yaz(unsigned x); \{ x tamsay\'fds\'fdn\'fdn 2li d\'fczendeki kar\'fe\'fdl\'fd\'f0\'fdn\'fd yazar \}
\par unsigned copybits(x, b, n)   \{ x say\'fds\'fdn\'fdn sa\'f0dan b. bitinden itibaren n bitini verir\}
\par }{unsigned ters(x, b, n):         \{ x}{\f68  say\'fds\'fdn\'fdn sa\'f0dan b. bitinden itibaren n bitini tersini al\'fdr\}
\par unsigned rdon(x,  n):          \{ x say\'fds\'fdn\'fd n bit sa\'f0a d\'f6nd\'fcr\'fcr\}
\par }{
\par 
\par }{\b\f68 /* Program\'fdn ba\'felang\'fdc\'fd */}{
\par }{\f68 typedef unsigned int word; /* kolayl\'fdk i.in */
\par }{
\par void binary_yaz(word);
\par word copybits(word x, word b, word  n);
\par word ters(word x, word b, word n);
\par word rdon(word x, word n);
\par word sdon(word x, word n);
\par 
\par main()
\par \{
\par   word  i, j;
\par 
\par   clrscr();
\par 
\par   for (i=16; i>0; i--)
\par       printf("%x", i-1);
\par   printf("\\n");
\par 
\par   i = 0xee;
\par   binary_yaz(i); printf("\\n");
\par   j = copybits(i, 5, 4);  binary_yaz(j); printf(" copybits(i, 5, 4) \\n");
\par 
\par   j = ters(i, 5, 4);  binary_yaz(j); printf(" ters(i, 5, 4)\\n");
\par 
\par   j = rdon(i, 4);  binary_yaz(j); printf(" rdon(i, 4)\\n");
\par 
\par   getch();
\par 
\par \}
\par }{\b word copybits(word x, word b, word  n)}{
\par /*}{\f68   x say\'fds\'fdn\'fdn sa\'f0dan b. bitinden itibaren n bitini verir.  */
\par /*  ilk bitin numaras\'fd 0  */
\par }{\{
\par   word i;
\par 
\par }{\f68   i = x >> (b + 1 - n);   /*  ilgili bit blo\'f0unun sa\'f0a dayal\'fd olacak  */
\par                             /*  bi\'e7imde kayd\'fdr.                         */
\par }{
\par }{\f68   i = i & ~(~(0) << n);   /*  sa\'f0daki n bit i\'e7in maske olu\'fetur  */
\par }{  return i;
\par \}   /*  End Of copybits  */
\par 
\par }{\b word ters(word x, word b, word n)}{
\par }{\f68 /*  x'in b. bitinden itibaren n bitin tersini al\'fdr  */
\par }{\{
\par   word p, r;
\par 
\par   p = ~(x);
\par   r = ~(~(0) << n) << (b - n + 1);   /*  se\'e7ilen bitler i\'e7in maske */
\par   p = p & r;
\par 
\par   x = x & ~(r);
\par   return x | p;
\par /*
\par 8 bit i\'e7in ters(. ,5,4)
\par       76543210
\par x = 0110 1000
\par p = 1001 0111    
\par r =  00111100    ~(~(0) << n) << (b - n + 1)
\par p = 00010100       p = p & r
\par x = 0100 0000      x & ~(r)
\par    = 01010100       x | p  d\'f6nen
\par \}   /*  End of test  */
\par 
\par }{\b word rdon(word x, word n)}{
\par }{\f68 /*  x'i n bit sa\'f0a d\'f6nd\'fcr\'fcr  */
\par }{\{
\par   word i;
\par   for (i = 1; i <= n; i++)
\par     \{
\par        if (x % 2 == 1)
\par           \{
\par             x = x / 2;
\par             x = x | 0x8000;
\par           \}
\par        else
\par           x = x / 2;
\par     \}
\par   return x;
\par \}   /*  End of rdon  */
\par 
\par }{\b word sdon(word x, word n)}{
\par }{\f68 /*  x'i n bit sa\'f0a kayd\'fdr\'fdr. RDon fonksiyonun ba\'feka bi\'e7imi */
\par }{\{
\par   word i;
\par 
\par   i = copybits(x,n - 1,n);
\par   x = x >> n;
\par   i = i << (16 - n + 1);
\par   return x | i;
\par \}   /*  End of RDon  */
\par 
\par }{\b void binary_yaz(word x)}{
\par \{
\par   int i;
\par   word m;
\par 
\par   m = 0x8000;
\par   for (i = 0; i<16; i++) \{
\par       if (x & m)
\par          printf("1");
\par       else
\par          printf("0");
\par       m = m >> 1;
\par   \}
\par \}
\par 
