\par       konum = i;
\par       for (j = 0; j < strlen(s2); j++)
\par           if (s1[i + j] != s2[j])
\par              konum = -1;
\par       if (konum >= 0)
\par          break;
\par       else i = i + 1;
\par     \}
\par   return konum; 
\par \}
